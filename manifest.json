{
  "manifest_version": 3,
  "name": "Channel Panel",
  "version": "1.0.0",
  "description": "Track YouTube channel statistics in one place.",
  "action": {
    "default_popup": "index.html",
    "default_title": "Channel Panel"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://www.googleapis.com/*"],
  "icons": {
    "16": "icon/icon-16.png",
    "32": "icon/icon-32.png",
    "48": "icon/icon-48.png",
    "128": "icon/icon-128.png"
  }
}
